<h2>Relatórios - Selecione uma Obra:</h2>
{{#each obras}}
<div class="card mt-4">
  <div class="card-body">
    <h4>{{nomeObra}}</h4>
    <small>Proprietário: {{nomeProprietario}}</small>
    <br>
    <small>Telefone: {{telefoneProprietario}}</small>
    <br>
    <small>Data de início: {{data}}</small>
    <br><br>
    <a href="/admin/relatorios/obra/{{_id}}"><button class="btn btn-success">Selecionar</button> </a>
  </div>
</div>
{{else}}
{{/each}}